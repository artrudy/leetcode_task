var smallerNumbersThanCurrent = function(nums) {
    let arrofNums = [...nums]
    arrofNums.sort((a, b) => a - b)
    let result = [];
    for (let i = 0; i < nums.length; i += 1){
        result.push(arrofNums.indexOf(nums[i]))
    }
     return result    
};
console.log(smallerNumbersThanCurrent([6,5,4,8]))
