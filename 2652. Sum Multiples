
var sumOfMultiples = function(n) {
  let arr = [];
  let result;
  for (let i = 1; i <= n; i += 1) {
    if (i % 3 === 0 || i % 5 === 0 || i % 7 === 0)
    arr.push(i)
  }
  if (arr.length > 0){
    result = arr.reduce((acc, rec) => acc + rec)
    return result;
  } else {
    result = 0
    return result;

  } 
  return result;
};

console.log(sumOfMultiples(10))
