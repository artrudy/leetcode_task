 var countPrimes = function(n) {
    let st = [];
    for (let i = 2; i < n; i += 1){
        for (let j = 2; j <= i; j += 1){
            if (j === i){
                st.push(i)
            }
            if (i % j === 0){
                break
            }

        }
    }
    return st.length
 }
    


console.log(countPrimes(499979))
