var longestCommonPrefix = function (strs) {
    const arr = strs.map(it => it.split(''))
    // console.log(arr)
    let result = arr[0]
    for (let i = arr[0].length - 1; i >= 0; i -= 1) {
        for (let j = arr.length - 1; j > 0; j -= 1) {
            // console.log(result[i], 'result[i]')
            // console.log(arr[j][i], 'arr[j][i]')
            if (result[i] !== arr[j][i]) {
                result.pop()
                break
            }
            // console.log(result, 'resultLoop')


        }


            }
            const emptyresult = "";

    if (result.length > 0) {
        return result.join('')
    } else {
        return emptyresult
    }
    // console.log(result.length)
    // const finalResult = result.length > 0 ? result.join(''): emptyresult
    // return finalResult

};
console.log(longestCommonPrefix(["cir","car"]))
