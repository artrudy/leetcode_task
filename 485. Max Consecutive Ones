var findMaxConsecutiveOnes = function(nums) {
    let max = 0;
    let count = 0;
    let resultArr = []
    for (let n = 0; n < nums.length; n += 1){
        if (nums[n] === 1) {
            count += 1;
            if (count > max){
                max = count
                }
            // count > max ? max = count: 0
            }
        
        if (nums[n] !== 1) {
            count = 0;
            }
        }
            
    return max    
};
