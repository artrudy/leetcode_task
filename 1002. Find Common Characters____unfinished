 var commonChars = function(A) {
        let res = [...A[0]];
        console.log(res)
        for (let i = 1; i < A.length; i++) {
          res = res.filter(it => {
            console.log('it', it)
            const l = A[i].length;
            console.log('A[i].length', A[i].length)
            console.log('A[i]', A[i])
            A[i] = A[i].replace(it, "");
            return l > A[i].length;
          });
        }
        return res;
      };

console.log(commonChars(["cool","lock","cook"]))
console.log('Output: [c, o]')
