var numIdenticalPairs = function(nums) {
  let counter = 0;
  for (let i = 0; i < nums.length; i += 1){
    for (j = i+1; j < nums.length; j += 1){
      if(nums[i] == nums[j]){
        // console.log('nums[i]', nums[i], 'i', i)
        // console.log('nums[j]', nums[j], 'j', j)
        counter += 1
      }
    }
  }
  return counter
    
};
console.log(numIdenticalPairs([1,2, 3]))
