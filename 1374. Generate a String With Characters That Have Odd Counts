var generateTheString = function(n) {
    let arr = new Array(n);
    if (n % 2 !== 0){
        for (let i = 0; i < n; i += 1){
            arr.push('a')
        }
    } else {
        for (let i = 0; i < n - 1; i += 1){
            arr.push('a')
        }
        arr.push('b')
    }
    return arr.join('')    
};
console.log(generateTheString(8))
